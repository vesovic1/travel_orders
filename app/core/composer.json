{
  "autoload": {
    "psr-4": {
      "TravelOrdersCore\\": "src/"
    }
  },
  "require": {
    "vlucas/phpdotenv": "^5.4",
    "robmorgan/phinx": "^0.12.11",
    "cakephp/database": "^4.4",
    "guzzlehttp/guzzle": "^7.5",
    "mpdf/mpdf": "^8.1",
    "cakephp/validation": "^4.4",
    "mailgun/mailgun-php": "^3.5",
    "phpoffice/phpspreadsheet": "^1.25",
    "psr/container": "^1.1",
    "league/flysystem": "^3.12",
    "predis/predis": "^2.1",
    "psr/event-dispatcher": "^1.0",
    "php-amqplib/php-amqplib": "^3.0",
    "knplabs/knp-snappy": "^1.5"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9"
  },
  "scripts": {
    "stan": "phpstan analyse --no-progress --ansi --memory-limit 1G"
  }
}
